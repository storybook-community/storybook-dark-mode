{
	"name": "storybook-dark-mode-monorepo",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"check": "biome check",
		"check:fix": "biome check --write",
		"clean": "turbo run clean",
		"cov": "turbo run cov",
		"cs": "changeset",
		"nuke": "turbo run nuke &&rimraf node_modules",
		"p:u": "playwright install --with-deps chromium",
		"release": "changeset publish",
		"sb": "pnpm run --filter=@storybook-community/storybook-dark-mode sb",
		"sb:build": "pnpm run --filter=@storybook-community/storybook-dark-mode sb:build",
		"test": "turbo run test",
		"v": "run-s check build cov",
		"verify": "run-s check build cov",
		"version": "changeset version"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@changesets/changelog-github": "^0.5.1",
		"@changesets/cli": "^2.29.7",
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@repobuddy/biome": "2.0.1",
		"@types/node": "^24.9.2",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"npm-run-all2": "^8.0.4",
		"playwright": "^1.58.2",
		"rimraf": "^6.1.0",
		"turbo": "^2.3.3"
	},
	"packageManager": "pnpm@10.30.1",
	"engines": {
		"node": ">=20.0.0",
		"pnpm": ">=10.0.0"
	}
}
