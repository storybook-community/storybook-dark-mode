{
	"name": "@storybook-community/storybook-dark-mode",
	"version": "6.0.3",
	"description": "Toggle between light and dark mode in Storybook",
	"keywords": [
		"storybook-addons",
		"appearance"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/storybook-community/storybook-dark-mode"
	},
	"license": "MIT",
	"author": {
		"name": "Andrew Lisowski",
		"email": "lisowski54@gmail.com"
	},
	"type": "module",
	"imports": {
		"#storybook-community/storybook-dark-mode": "./src/index.tsx",
		"#storybook-community/storybook-dark-mode/docs": "./src/docs/index.ts"
	},
	"exports": {
		".": {
			"import": {
				"types": "./esm/index.d.ts",
				"default": "./esm/index.js"
			},
			"require": {
				"types": "./cjs/index.d.ts",
				"default": "./cjs/index.js"
			}
		},
		"./manager": {
			"import": {
				"types": "./esm/manager/index.d.ts",
				"default": "./esm/manager/index.js"
			},
			"require": {
				"types": "./cjs/manager/index.d.ts",
				"default": "./cjs/manager/index.js"
			}
		},
		"./docs": {
			"import": {
				"types": "./esm/docs/index.d.ts",
				"default": "./esm/docs/index.js"
			},
			"require": {
				"types": "./cjs/docs/index.d.ts",
				"default": "./cjs/docs/index.js"
			}
		},
		"./preset": "./preset.js",
		"./package.json": "./package.json"
	},
	"main": "./cjs/index.js",
	"module": "./esm/index.js",
	"types": "./esm/index.d.ts",
	"files": [
		"cjs",
		"esm",
		"src",
		"preset.cjs",
		"preset.js",
		"!**/*.{spec,stories}.*"
	],
	"scripts": {
		"prebuild": "yarn clean",
		"build": "unbuild && ncp ./package.cjs.json ./cjs/package.json",
		"clean": "rimraf cjs dist esm",
		"cs": "changeset",
		"lint": "biome check",
		"prepack": "unbuild && ncp ./package.cjs.json ./cjs/package.json",
		"release": "changeset publish",
		"version": "changeset version",
		"sb": "storybook dev",
		"sb:build": "cross-env NODE_ENV=production storybook build",
		"test": "vitest run",
		"watch": "vitest"
	},
	"dependencies": {
		"@storybook/icons": "^1.4.0",
		"fast-deep-equal": "^3.1.3"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@changesets/changelog-github": "^0.5.1",
		"@changesets/cli": "^2.28.1",
		"@commitlint/cli": "^19.8.0",
		"@commitlint/config-conventional": "^19.8.0",
		"@repobuddy/biome": "1.7.1",
		"@repobuddy/storybook": "^1.0.1",
		"@storybook/addon-docs": "9.0.18",
		"@storybook/addon-vitest": "9.0.18",
		"@storybook/react-vite": "9.0.18",
		"@tailwindcss/vite": "^4.1.10",
		"@types/node": "^18.11.11",
		"@types/react": "^18.0.26",
		"@vitest/browser": "^3.2.3",
		"all-contributors-cli": "^6.24.0",
		"cross-env": "^10.0.0",
		"ncp": "^2.0.0",
		"npm-run-all2": "^8.0.4",
		"playwright": "*",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"rimraf": "^3.0.2",
		"storybook": "9.0.18",
		"storybook-addon-tag-badges": "^2.0.2",
		"tailwindcss": "^4.1.10",
		"typescript": "^5.7.3",
		"unbuild": "^3.5.0",
		"vite": "^6.3.5",
		"vitest": "^3.2.3"
	},
	"peerDependencies": {
		"@storybook/addon-docs": "^9.0.18",
		"storybook": "^9.0.18"
	},
	"peerDependenciesMeta": {
		"@storybook/addon-docs": {
			"optional": true
		}
	},
	"packageManager": "yarn@1.22.22"
}
