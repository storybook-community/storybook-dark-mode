{
	"name": "@storybook-community/storybook-dark-mode",
	"version": "6.0.8",
	"description": "Toggle between light and dark mode in Storybook",
	"keywords": [
		"storybook-addon",
		"appearance",
		"dark-mode",
		"dark",
		"theme"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/storybook-community/storybook-dark-mode"
	},
	"license": "MIT",
	"author": {
		"name": "Andrew Lisowski",
		"email": "lisowski54@gmail.com"
	},
	"type": "module",
	"imports": {
		"#storybook-community/storybook-dark-mode": "./src/index.ts",
		"#storybook-community/storybook-dark-mode/docs": "./src/docs.ts"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./preview": {
			"types": "./dist/preview.d.ts",
			"default": "./dist/preview.js"
		},
		"./manager": {
			"types": "./dist/manager.d.ts",
			"default": "./dist/manager.js"
		},
		"./docs": {
			"types": "./dist/docs.d.ts",
			"default": "./dist/docs.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist/**/*",
		"README.md",
		"*.js",
		"*.d.ts"
	],
	"scripts": {
		"build": "tsup",
		"build:watch": "yarn build --watch",
		"clean": "rimraf dist",
		"cs": "changeset",
		"lint": "biome check",
		"prebuild": "yarn clean",
		"prepack": "yarn build",
		"release": "changeset publish",
		"version": "changeset version",
		"sb": "storybook dev",
		"sb:build": "cross-env NODE_ENV=production storybook build",
		"storybook": "storybook dev -p 6006",
		"test": "vitest run",
		"watch": "vitest"
	},
	"dependencies": {
		"@storybook/icons": "^1.6.0",
		"fast-deep-equal": "^3.1.3"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.2",
		"@changesets/changelog-github": "^0.5.1",
		"@changesets/cli": "^2.29.7",
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@repobuddy/biome": "2.0.1",
		"@repobuddy/storybook": "^1.0.1",
		"@storybook/addon-docs": "next",
		"@storybook/addon-vitest": "next",
		"@storybook/react-vite": "next",
		"@tailwindcss/vite": "^4.1.10",
		"@types/node": "^24.9.2",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@vitest/browser": "^4.0.5",
		"all-contributors-cli": "^6.24.0",
		"cross-env": "^10.0.0",
		"ncp": "^2.0.0",
		"npm-run-all2": "^8.0.4",
		"playwright": "*",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"rimraf": "^6.0.1",
		"storybook": "next",
		"storybook-addon-tag-badges": "^3.0.1",
		"tailwindcss": "^4.1.10",
		"tsup": "^8.5.0",
		"typescript": "^5.7.3",
		"unbuild": "^3.5.0",
		"vite": "^7.1.12",
		"vitest": "^4.0.5"
	},
	"peerDependencies": {
		"@storybook/addon-docs": "^10.0.0",
		"storybook": "^10.0.0"
	},
	"peerDependenciesMeta": {
		"@storybook/addon-docs": {
			"optional": true
		}
	},
	"bundler": {
		"managerEntries": [
			"src/manager.tsx"
		],
		"previewEntries": [
			"src/docs.ts",
			"src/preview.ts",
			"src/index.ts"
		],
		"nodeEntries": []
	},
	"packageManager": "yarn@1.22.22",
	"storybook": {
		"displayName": "Dark mode (community version)",
		"supportedFrameworks": [
			"supported-frameworks"
		],
		"icon": "https://avatars.githubusercontent.com/u/216219414?s=200&v=4"
	}
}
