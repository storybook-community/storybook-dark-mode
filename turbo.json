{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"clean": {
			"cache": false
		},
		"cov": {
			"dependsOn": ["^build"],
			"outputs": ["coverage/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"nuke": {
			"cache": false
		},
		"test": {
			"dependsOn": ["^build"],
			"outputs": ["coverage/**"]
		}
	}
}
